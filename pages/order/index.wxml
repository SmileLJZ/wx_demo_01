<Tabs tabs="{{tabs}}" bindtabsItemChange="handleTabsItemChange">
    <view class="order_main">
        <view class="order_item" wx:for="{{orders}}" wx:key="order_id">
            <view class="order_row order_no_row">
                <view class="order_text">订单编号</view>
                <view class="order_value">{{item.order_number}}</view>
            </view>
            <view class="order_row order_price_row">
                <view class="order_text">订单价格</view>
                <view class="order_value">￥{{item.order_price}}</view>
            </view>
            <view class="order_row order_time_row">
                <view class="order_text">订单日期</view>
                <view class="order_value">{{item.new_create_time}}</view>
            </view>
        </view>
    </view>
</Tabs>